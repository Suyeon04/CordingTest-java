package Level1;

class Solution {
    public long[] solution(int x, int n) {
        long[] answer = {};
        answer = new long[n]; 
        long num = x;
        for(int i = 0; i< n; i++){
                answer[i] = num;
                num += x;
        }
        return answer;
    }
}
public class interval_XtoBN {
	public static void main(String args[]) {
		Solution s = new Solution();
		long a[] = s.solution(2,5);
		
	}
}

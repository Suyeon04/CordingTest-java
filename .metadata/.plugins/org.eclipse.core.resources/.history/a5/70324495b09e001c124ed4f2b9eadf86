package CosPro1;

public class atoi {
	    public int solution(String s) {
	        int answer = 0;
	        String a[] = s.split("");
	        int x = 10;
	        for(int i = 0; i<a.length; i++,x*=10){
	            answer += Integer.parseInt(a[i]);
	            System.out.println(answer);
	            answer*=x;
	        }
	        System.out.println(answer);
	            return answer;
	    }
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		atoi a = new atoi();
		System.out.println(a.solution("1203"));
	}

}

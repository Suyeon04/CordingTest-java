package CosPro1;

public class 수박 {
	public String solution(int n) {
        String answer = "";
        int x =0;
        for(int i = 0; i<n;i++){
            if(x==0){
                answer+="수";
                x++;
            }else{
                answer+="박";
                x--;
            }     
        }
        return answer;
    }
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		수박 a = new 수박();
		String b = a.solution(3);
	}

}

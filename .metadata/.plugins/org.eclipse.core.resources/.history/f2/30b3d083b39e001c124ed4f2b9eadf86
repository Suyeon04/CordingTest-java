package CosPro1;

public class Áø¼ö {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 77;
		int i = 2;
		int answer = 0;
		int x = 1;
		for(i = 1; a>=i;i*=2) {
			x*=10;
		}
		i/=2;
		if(a%2==0) {
			answer++;
		}
		for(int j = i; j!=0; j/=2) {
			if(a>j) {
				answer += x;
				a %= j;
				System.out.println(answer);
			}
			
		}
	}
}
